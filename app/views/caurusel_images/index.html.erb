<p id="notice"><%= notice %></p>

<h1>Listing Caurusel Images</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Img</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @caurusel_images.each do |caurusel_image| %>
      <tr>
        <td><%= caurusel_image.name %></td>
        <td><%= caurusel_image.img %></td>
        <td><%= link_to 'Show', caurusel_image %></td>
        <td><%= link_to 'Edit', edit_caurusel_image_path(caurusel_image) %></td>
        <td><%= link_to 'Destroy', caurusel_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Caurusel image', new_caurusel_image_path, class: 'btn btn-success' %>
